{
  "name": "yoummday/refactoring",
  "description": "Coding interview refactoring task",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Philipp Lohmann",
      "email": "philipp.lohmann@yoummday.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "ext-yaml": "*",
    "progphil1337/simple-react-app": "dev-main"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "guzzlehttp/guzzle": "^7.8",
    "symfony/process": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App/"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "Test/"
    },
    "classmap": [
      "Test/"
    ]
  }
}
